		"graphql-yoga": "^5.15.1",
		"reflect-metadata": "^0.2.2",
		"type-graphql": "^2.0.0-rc.2"
	},
	"devDependencies": {
		"@types/bun": "^1.2.20",
		"@types/node": "^24.3.0",
		"prisma": "^6.14.0",
		"typescript": "^5.9.2",
		"zenstack": "^2.18.1"
	}
}