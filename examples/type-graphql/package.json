{
	"name": "type-graphql",
	"version": "1.0.0",
	"description": "TypeGraphQL test example for ZenStack GraphQL plugin",
	"main": "src/server.ts",
	"scripts": {
		"generate": "zenstack generate",
		"db:setup": "node scripts/generate-prisma.js",
		"db:migrate": "prisma migrate dev",
